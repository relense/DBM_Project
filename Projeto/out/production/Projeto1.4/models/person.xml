<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE model SYSTEM "../metamodels/model.dtd">
<model name="Person">
    <class name="Person">
        <attribute name="name" type="String" required="true" />
        <attribute name="age" type="int" required="true"/>
        <relation name="car" type="1ToN" />
    </class>
    <class name="Car">
        <attribute name="name" type="String" />
        <attribute name="year" type="int" />
        <attribute name="model" type="String" />
        <attribute name="brand" type="String" />
        <relation name="person" type="NToN" />
    </class>
</model>

